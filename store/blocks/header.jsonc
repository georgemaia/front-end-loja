			"id": "menu-item-produtos",
			"type": "custom",
			"highlight": false,
			"itemProps": {
				"type": "internal",
				"href": "#",
				"noFollow": false,
				"tagTitle": "Produtos",
				"text": "Produtos"
			},
			"iconToTheRight": true
		}
	},
